datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Booking {
  id           Int      @id @default(autoincrement())
  customerName String
  date         DateTime
  time         String
  guests       Int
  seatNumber   String   
  status       String   @default("Pending")
  createdAt    DateTime @default(now())
}